{
    "compilerOptions": {
        "rootDir": "source/",
        "outDir": "dist/",
        "declaration": true,
        "target": "ES2017",
        "module": "CommonJS",
        "moduleResolution": "Node",
        "esModuleInterop": true,
        "jsx": "react",
        "jsxFactory": "createCommand",
        "importHelpers": true,
        "lib": ["ES2019", "DOM"]
    },
    "typedocOptions": {
        "name": "Hexo Wiki creator",
        "excludeExternals": true,
        "excludePrivate": true,
        "readme": "./ReadMe.md",
        "plugin": ["typedoc-plugin-mdn-links"]
    }
}
